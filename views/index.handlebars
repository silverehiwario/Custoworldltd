      $.ajax({ url: currentURL + "/index", method: "GET" })
      .done(function(imageData) {
        // Here we are logging the URL so we have access to it for troubleshooting

     // Here we then log the NYTData to console, where it will show up as an object.
        console.log(imageData);
        // Loop through and display each of the customers





        for (var i = 0; i < imageData.length; i++) {
          // Create the HTML Well (Section) and Add the table content for each reserved table

          $("#thumb1").append("<a target =_blank href="+ imageData[i].input_website + "><img src = images/upload_images/"  + imageData[i].upload_image +  "></a>");

           console.log("<a href=" + imageData[i].input_website + "><img src = images/upload_images/"  + imageData[i].upload_image +  "></a>");
         }
          });
    }
